          <ul id="roles">
            {{#each roles}}
              <li class="grey-text text-lighten-4 rolesubmit roletitle">
                <h5 style="color:blue" class="roletitle">{{name}}</h5>
                <form class="roleidcontainer" action="/roledetails" method="POST">
                  <input type="hidden" name="roleid" value="{{roleid}}" />
                </form>
              </li>
            {{/each}}
          </ul>

      </div>
    </div>

